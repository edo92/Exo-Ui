{
  "name": "exo-ui",
  "version": "0.0.2",
  "main": "build/index.cjs.js",
  "module": "build/index.esm.js",
  "browser": "build/index.js",
  "typings": "build/src/index.d.ts",
  "files": [
    "build",
    "README.md"
  ],
  "scripts": {
    "dev": "cross-env TS_NODE_PROJECT=\"./config/webpack.tsconfig.json\" webpack s --config ./webpack.config.dev.ts",
    "rollup": "rollup -c",
    "build": "npm run rollup",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "test": "cypress open-ct --browser $browser",
    "test:chrome": "cypress run-ct --config video=false --browser chrome",
    "test:firefox": "cypress run-ct --config-file ./cypress.json --config video=false --browser firefox",
    "test:edge": "cypress run-ct --config-file ./cypress.json --config video=false --browser edge",
    "prepare": "npm run build",
    "setup": "npm install --ignore-scripts"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/plugin-transform-runtime": "^7.16.4",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-react": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@babel/runtime": "^7.16.3",
    "@cypress/react": "^5.10.3",
    "@cypress/webpack-dev-server": "^1.7.0",
    "@cypress/webpack-preprocessor": "^5.10.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@rollup/plugin-url": "^6.1.0",
    "@svgr/rollup": "^6.0.0",
    "@types/color": "^3.0.2",
    "@types/node": "^16.11.10",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@types/react-router-dom": "^5.3.2",
    "@types/styled-components": "^5.1.15",
    "@types/webpack": "^5.28.0",
    "@types/webpack-dev-server": "^4.5.0",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.1",
    "babel-loader": "^8.2.3",
    "color": "^4.0.2",
    "cross-env": "^7.0.3",
    "css-loader": "^6.5.1",
    "cypress": "^9.1.0",
    "execa": "^6.0.0",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^6.4.2",
    "html-loader": "^3.0.1",
    "html-webpack-plugin": "^5.5.0",
    "react-router": "^6.0.2",
    "regenerator-runtime": "^0.13.9",
    "rollup": "^2.60.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.31.1",
    "style-loader": "^3.3.1",
    "styled-components": "^5.3.3",
    "svg-url-loader": "^7.1.1",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2",
    "typescript-styled-plugin": "^0.18.1",
    "webpack": "^5.64.4",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.6.0"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.0.2"
  },
  "repository": {
